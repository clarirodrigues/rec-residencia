<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:tools="http://schemas.android.com/tools">
    <data>
        <variable name="staggeredeRowModel" type="com.victriasapplication.app.modules.msicaacordes.data.model.StaggeredeRowModel"/>
    </data>
    <LinearLayout android:gravity="center_horizontal"
        android:id="@+id/linearColumne"
        android:layout_height="wrap_content"
        android:layout_width="match_parent" android:orientation="vertical">
        <LinearLayout android:id="@+id/linearRowe"
            android:layout_height="wrap_content"
            android:layout_marginEnd="@dimen/_1pxh"
            android:layout_marginStart="@dimen/_1pxh"
            android:layout_marginTop="@dimen/_1pxv"
            android:layout_width="match_parent" android:orientation="horizontal">
            <ImageView android:id="@+id/imageE"
                android:layout_height="@dimen/_8pxh"
                android:layout_marginBottom="@dimen/_1pxv"
                android:layout_marginTop="@dimen/_1pxv"
                android:layout_width="@dimen/_8pxh"
                android:scaleType="centerCrop" circular="@{true}"
                imageRes="@{@drawable/img_e}"
                tools:ignore="ContentDescription" tools:src="@drawable/img_e"/>
            <ImageView android:id="@+id/imageA"
                android:layout_height="@dimen/_8pxh"
                android:layout_marginStart="@dimen/_10pxh"
                android:layout_width="@dimen/_8pxh"
                android:scaleType="centerCrop" circular="@{true}"
                imageRes="@{@drawable/img_a}"
                tools:ignore="ContentDescription" tools:src="@drawable/img_a"/>
            <ImageView android:id="@+id/imageD"
                android:layout_height="@dimen/_8pxh"
                android:layout_marginStart="@dimen/_9pxh"
                android:layout_width="@dimen/_8pxh"
                android:scaleType="centerCrop" circular="@{true}"
                imageRes="@{@drawable/img_a}"
                tools:ignore="ContentDescription" tools:src="@drawable/img_a"/>
            <ImageView android:id="@+id/imageG"
                android:layout_height="@dimen/_8pxh"
                android:layout_marginStart="@dimen/_9pxh"
                android:layout_width="@dimen/_8pxh"
                android:scaleType="centerCrop" circular="@{true}"
                imageRes="@{@drawable/img_a}"
                tools:ignore="ContentDescription" tools:src="@drawable/img_a"/>
            <ImageView android:id="@+id/imageB"
                android:layout_height="@dimen/_8pxh"
                android:layout_marginStart="@dimen/_9pxh"
                android:layout_width="@dimen/_8pxh"
                android:scaleType="centerCrop" circular="@{true}"
                imageRes="@{@drawable/img_a}"
                tools:ignore="ContentDescription" tools:src="@drawable/img_a"/>
            <ImageView android:id="@+id/imageEOne"
                android:layout_height="@dimen/_8pxh"
                android:layout_marginStart="@dimen/_9pxh"
                android:layout_width="@dimen/_8pxh"
                android:scaleType="centerCrop" circular="@{true}"
                imageRes="@{@drawable/img_a}"
                tools:ignore="ContentDescription" tools:src="@drawable/img_a"/>
        </LinearLayout>
        <FrameLayout android:id="@+id/frameStackfretboard"
            android:layout_gravity="center_horizontal"
            android:layout_height="@dimen/_132pxv"
            android:layout_marginEnd="@dimen/_4pxh"
            android:layout_marginStart="@dimen/_4pxh"
            android:layout_marginTop="@dimen/_4pxv" android:layout_width="@dimen/_94pxh">
            <ImageView android:id="@+id/imageFretboard"
                android:layout_gravity="start"
                android:layout_height="@dimen/_132pxh"
                android:layout_width="@dimen/_94pxh"
                android:scaleType="fitXY"
                android:src="@drawable/img_fretboard"
                tools:ignore="ContentDescription" tools:src="@drawable/img_fretboard"/>
            <LinearLayout android:id="@+id/linearRowrectanglenine"
                android:layout_gravity="top|end"
                android:layout_height="wrap_content"
                android:layout_marginBottom="@dimen/_10pxv"
                android:layout_marginEnd="@dimen/_13pxh"
                android:layout_marginStart="@dimen/_13pxh"
                android:layout_marginTop="@dimen/_10pxv"
                android:layout_width="wrap_content" android:orientation="horizontal">
                <View
                    android:background="@drawable/rectangle_bg_blue_a700_border_blue_a700_radius_5_335"
                    android:id="@+id/viewRectangleNine"
                    android:layout_height="@dimen/_10pxh"
                    android:layout_marginTop="@dimen/_25pxv" android:layout_width="@dimen/_10pxh"/>
                <View
                    android:background="@drawable/rectangle_bg_blue_a700_border_blue_a700_radius_5_335"
                    android:id="@+id/viewRectangleNineOne"
                    android:layout_height="@dimen/_10pxh"
                    android:layout_marginStart="@dimen/_8pxh"
                    android:layout_marginTop="@dimen/_25pxv" android:layout_width="@dimen/_10pxh"/>
                <View
                    android:background="@drawable/rectangle_bg_blue_a700_border_blue_a700_radius_5_335"
                    android:id="@+id/viewRectangleNineTwo"
                    android:layout_height="@dimen/_10pxh"
                    android:layout_marginBottom="@dimen/_25pxv"
                    android:layout_marginStart="@dimen/_8pxh" android:layout_width="@dimen/_10pxh"/>
            </LinearLayout>
        </FrameLayout>
        <TextView android:ellipsize="end" android:id="@+id/txtAOne"
            android:layout_gravity="center_horizontal"
            android:layout_height="wrap_content"
            android:layout_marginEnd="@dimen/_30pxh"
            android:layout_marginStart="@dimen/_30pxh"
            android:layout_marginTop="@dimen/_13pxv"
            android:layout_width="wrap_content"
            android:singleLine="true"
            android:text="@{staggeredeRowModel.txtAOne}"
            style="@style/txtInterbold24" tools:text="@string/lbl_am"/>
    </LinearLayout>
</layout>
