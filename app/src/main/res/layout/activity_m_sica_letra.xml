                        android:paddingHorizontal="@dimen/_23pxh"
                        android:paddingVertical="@dimen/_23pxv"
                        android:scaleType="fitCenter"
                        android:src="@drawable/img_vector_white_a700_79x79" tools:ignore="ContentDescription"/>
                </FrameLayout>
            </FrameLayout>
            <LinearLayout android:gravity="center_vertical"
