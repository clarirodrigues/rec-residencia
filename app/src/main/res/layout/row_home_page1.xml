<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:tools="http://schemas.android.com/tools">
    <data>
        <variable name="homePage1RowModel" type="com.victriasapplication.app.modules.homepage.data.model.HomePage1RowModel"/>
    </data>
    <LinearLayout android:id="@+id/linearRowellipseone"
        android:layout_height="wrap_content"
        android:layout_width="match_parent"
        android:orientation="horizontal" style="@style/groupStyleteal_50cornerRadius">
        <ImageView android:id="@+id/imageEllipseOne"
            android:layout_height="@dimen/_44pxh"
            android:layout_marginBottom="@dimen/_4pxv"
            android:layout_marginStart="@dimen/_8pxh"
            android:layout_marginTop="@dimen/_4pxv"
            android:layout_width="@dimen/_44pxh"
            android:scaleType="centerCrop" circular="@{true}"
            imageRes="@{@drawable/img_ellipse1}"
            tools:ignore="ContentDescription" tools:src="@drawable/img_ellipse1"/>
        <TextView android:ellipsize="end"
            android:id="@+id/txtBobMarleyOne"
            android:layout_height="wrap_content"
            android:layout_marginBottom="@dimen/_15pxv"
            android:layout_marginEnd="@dimen/_28pxh"
            android:layout_marginStart="@dimen/_8pxh"
            android:layout_marginTop="@dimen/_19pxv"
            android:layout_weight="0.63" android:layout_width="0dp"
            android:singleLine="true"
            android:text="@{homePage1RowModel.txtBobMarleyOne}"
            style="@style/txtIntermedium14_1" tools:text="@string/lbl_bob_marley"/>
    </LinearLayout>
</layout>
